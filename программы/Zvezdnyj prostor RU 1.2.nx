'NAZVANIE:   ZVEZDNYJ PROSTOR
'AVTORY:     TIMO KLOSS, DESBYC
'PEREVOD:    TREEBIT

DIM GLOBAL STARX(127)

FOR I=0 TO 127
  STARX(I)=INT(RND*256)
NEXT I
BG 1
BG FILL 0,0 TO 0,15 CHAR 3

GLOBAL SX,T

ON RASTER CALL RASTERFX

T$="DOBRO POXALOVAT' V 1O23E4 56! VIRTUAL'NUW IGROVUW RETRO PRISTAVKU,PROGRAMMIRUEMUW NA KLASSIQESKOM BEJSIKE. UDAQI V TVORENII! ... KOD I ZVUK. CFFEKTY OT T7MO, MUZYKA OT 8E4V9S. PRIVET VSEM 1O23E4 56 KODERAM!                     "

CELL SIZE 0,1
BG 0
PAL 1
PRIO 1
P=1
L=LEN(T$)

MUSIC

DO
  IF SX MOD 16=0 THEN
    P$=MID$(T$,P,1)
    IF P$>="A" AND P$<="Z" THEN
      N=ASC(P$)-ASC("A")
      C=(N\8)*32+(N MOD 8)*2+64
    ELSE
      IF P$="." THEN C=164
      IF P$="," THEN C=166
      IF P$="!" THEN C=168
      IF P$="?" THEN C=170
      IF P$=" " THEN C=174
      IF P$="'" THEN C=78
      IF P$>="1" THEN C=4
      IF P$>="2" THEN C=6
      IF P$>="3" THEN C=8
      IF P$>="4" THEN C=10
      IF P$>="5" THEN C=12
      IF P$>="6" THEN C=14
      IF P$>="7" THEN C=42
      IF P$>="8" THEN C=44
      IF P$>="9" THEN C=46
    END IF
    CELL SX/16+10,0,C
    P=(P MOD L)+1
  END IF
 
  SCROLL 0,SX,SIN(T*0.02)*32-56
 
  P2=PEEK($FF5B)
 
  FOR I=0 TO 31
    X1=SIN(T*0.02+I*PI/16)
    Y1=COS(T*0.03+I*PI/16)
    X2=COS(T*0.045+I*0.09)
    Y2=SIN(T*0.035+I*0.1)
    X3=SIN(T*0.025+I*0.12)
    Y3=COS(T*0.015+I*0.13)
  
    Z=1+P2/1536
    X=(X1*X2*60+X3*32)*Z
    Y=(Y1*Y2*60+Y3*32)*Z
    SPRITE I PAL 2+(I/4) MOD 2
    SPRITE I,X+76,Y+60,2
  NEXT I
 
  T=T+1
  SX=SX+1
  WAIT VBL
LOOP

SUB RASTERFX
  F=0.5
  D=((RASTER MOD 4)+1)
  SCROLL 1,TIMER*F/D+STARX(RASTER),0
END SUB


